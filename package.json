{
  "name": "fullstack-js-start",
  "version": "1.0.0",
  "description": "Simple starter template for build fullstack js applications using MERN stack",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "cd client && npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yugako/fullstack-js-start.git"
  },
  "keywords": [
    "MERN",
    "Node",
    "Fullstack",
    "Boilerplate"
  ],
  "author": "George Hasiuk",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yugako/fullstack-js-start/issues"
  },
  "homepage": "https://github.com/yugako/fullstack-js-start#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "config": "^3.2.4",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "express-validator": "^6.3.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.8.3"
  },
  "devDependencies": {
    "concurrently": "^5.0.2",
    "nodemon": "^2.0.2"
  }
}
